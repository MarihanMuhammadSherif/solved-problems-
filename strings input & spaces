
#include <stdio.h>
/*
scanf cannt recognize spaces 
so we must use "%[^\n]s"
fgets does recognise spaces

*/ 
int main()
{
     //reading  a string with spaces  using
     int i;
char A[30];
////////////////////
//1.scanf
scanf("%s",A);
for(i=0;A[i] !=0;++i)
{

          printf("%c %d \n",A[i],A[i]);
}
/////////////////////////
//2. scanf with [^\n]
scanf("%[^\n]s",A);
for(i=0;A[i] !=0;++i)
{

          printf("%c %d \n",A[i],A[i]);
}
///////////////////////
//3.fgets
fgets(A,30,stdin);

for(i=0;A[i] !=0;++i)
{

          printf("%c %d \n",A[i],A[i]);
}



    return 0;
}

///function to calculate the number of words in a string


int counter_words(char* a){`

 // go through chars in a
 // if ' ' new word
 int words=1;
 int i;
 for(i=0;i<strlen(a);++i)
 {
      if(a[i]==' ' && a[i+1] !=0)
      {
           ++words;
      }
 }
return words;}


///function to encode a string 
char* encoder(char* s)
{
     int i;
     for(i=0;i<strlen(s);++i)
     {
          s[i]-=30;
     }
     return s;
}


///find a substring in a string 
int searchsub(char* b,char* a );
int main()
{
 char a[50];
 char b[50];
 fgets(a,50,stdin);
 fgets(b,50,stdin);
 int c=searchsub(b,a);
if(c!=0)
{
     printf("yes %d",c);
}
else
      printf("no bitch");


    return 0;
}
int searchsub(char* b,char* a )
{     //check
   printf("%d \n",strlen(a));
 printf("%d \n",strlen(b));
     int i;
     for(i=0;i<strlen(a);++i)
     {
          if(a[i]==b[0])
          {
               int j;
               int k=i+1;
               int flag=1;
               for( j=1 ; j<strlen(b)-1 ; ++j , ++k )
               {
                    if(a[k] != b[j])
                    {
                         flag=0;
                    }
               }
               if(flag==1)
                    return i+1;
          }
     }

     return 0;

}

/// reverse a string 
int main()
{
  char a[100];
  fgets(a,100,stdin);
  char temp;
  int i,j;
  j=strlen(a)-1;
  for(i=0 ; i<j ; ++i ,--j)
  {
       temp=a[i];
       a[i]=a[j];
       a[j]=temp;

  }
  printf("%s",a);

    return 0;
}


// program to sort strings alphabiticaly 
int main()
{     char a[5][20];

     int i,j;
     for(i=0 ; i<5 ;++i)
     {          fgets(a[i],20,stdin);
     }
    char t[20];


     //each element is compared to all others
     //5 elements
     // 4 passes
     // 4 comparisons for each pass

  /  for(i=0 ; i<4 ;++i )
     {
          for(j=0 ; j<4 ;++j)
          {
               if(strcmp(a[j] ,a[j+1]) >0)
               {     strcpy(t,a[j]);
                strcpy(a[j],a[j+1]);
                 strcpy(a[j+1],a[j]);
               }

          }
     }



       for(i=0 ; i<5 ;++i)
     {          printf("%s",a[i]);
     }

    return 0;
}
